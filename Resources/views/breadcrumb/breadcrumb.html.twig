{% if breadcrumb is defined %}
	<section class="content-header">
	  	<h1>
	  		{% for item in breadcrumb %}	    	
		    	{% if loop.first %}
		      		{{ item }} »
			    {% elseif loop.last %}
			       	<small> {{ item }} </small>
			    {% else %}
			      	<small> {{ item }} » </small>
			    {% endif %}
	    	{% endfor %}
	  	</h1>	  	
	</section>
	<br>
{% endif %}
